{
  "version": 2,
  "builds": [
    {
      "src": "api/kalkulator.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/kalkulator",
      "dest": "api/kalkulator.js"
    },
    {
      "src": "/form",
      "dest": "/public/form.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
