{
  "version": 2,
  "builds": [
    {
      "src": "api/kalkulator.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/kalkulator",
      "dest": "api/kalkulator.js"
    },
  ]
  {
  "rewrites": [
    { "source": "/", "destination": "/public/index.html" },
    { "source": "/(.*)", "destination": "/public/$1" }
  ]
}
}
